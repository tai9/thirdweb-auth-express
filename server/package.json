{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --files src/index.ts",
    "start": "nodemon dist/index.js",
    "build-ts": "tsc",
    "build": "yarn run build-ts",
    "typeorm": "typeorm-ts-node-esm -d ./src/configs/db.config.ts",
